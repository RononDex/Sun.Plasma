<Window x:Class="Sun.Plasma.UpdateSelfUpdater"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="UpdateSelfUpdater" Width="400" Height="160" Style="{StaticResource SunWindow}">
<GroupBox>
        <StackPanel Orientation="Vertical">
            <Label Style="{StaticResource LblTitle}">Updating SelfUpdater. Please wait...</Label>
            <ProgressBar Name="CtlProgress" HorizontalAlignment="Stretch" Margin="5" Height="30" />
        </StackPanel>
    </GroupBox>
</Window>
